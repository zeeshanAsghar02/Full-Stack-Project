{
  "name": "auis-nexus",
  "version": "1.0.0",
  "description": "Air University Islamic Society (AUIS) Website",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd frontend && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "AUIS",
    "Islamic Society",
    "MERN Stack",
    "React",
    "Node.js",
    "Express",
    "MongoDB"
  ],
  "author": "AUIS Development Team",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "imagekit": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.1.10"
  }
}
